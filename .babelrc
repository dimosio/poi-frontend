{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    "syntax-dynamic-import",
    "transform-decorators-legacy",
    "lodash",
    ["import", { "libraryName": "antd", "style": true }],
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}
